{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": ["node_modules/**/*ts"]
    },
    "jsRules": {},
    "rules": {
        "no-console":false,
        "no-debugger":false,
        "ordered-imports":false,
        "object-literal-sort-keys":false,
        "jsx-no-lambda":false,
        "member-ordering": [false, {"order": "fields-first"}]
    },
    "rulesDirectory": []
}